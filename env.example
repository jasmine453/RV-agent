# RV-Agent 环境变量配置示例
# 复制此文件为 .env 并填入实际的配置值

# 服务器配置
PORT=3000
HOST=localhost
NODE_ENV=development

# DeepSeek AI API配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# 豆包（火山引擎）图片生成API配置
DOUBAO_API_KEY=your_doubao_api_key_here

# 日志配置
LOG_LEVEL=info

# CORS配置（生产环境应设置具体域名）
CORS_ORIGIN=*

# 文件上传配置
MAX_FILE_SIZE=10485760
UPLOAD_DIR=uploads

# 其他配置
# 设置时区
TZ=Asia/Shanghai

